<h1>Sign in</h1>
  <%= form_for '', url: {action: "create"}, html: {class: "user_form"} do |f| %>
      <div class="form-group">
        <%= f.email_field :email, class: "form-control", placeholder: "email" %>
      </div>
      <div class="form-group">
        <%= f.text_field :password_digest, class: "form-control", placeholder: "password" %>
      </div>
      <div class="form-group">
        <%= f.submit "Sign in", class: "btn, btn-default" %>
      </div>
  <% end %>